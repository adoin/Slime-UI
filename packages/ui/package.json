  "devDependencies": {
    "@mylib/typescript-config": "workspace:*",
    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/runtime-core": "^3.5.13",
    "sass": "^1.81.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "style": "dist/styles/index.css"
